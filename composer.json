{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "alazzi-az/laravel-bitmask": "^1.0",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "intervention/image": "^3.9",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9",
        "sentry/sentry-laravel": "^4.10",
        "spatie/image-optimizer": "^1.8",
        "spatie/laravel-honeypot": "^4.5",
        "spatie/laravel-medialibrary": "^11.10",
        "spatie/laravel-sluggable": "^3.6",
        "watheqalshowaiter/backup-tables": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "driftingly/rector-laravel": "^1.2",
        "fakerphp/faker": "^1.23",
        "jasonmccreary/laravel-test-assertions": "^2.4",
        "laravel-shift/blueprint": "^2.10",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.3",
        "pestphp/pest-plugin-faker": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "rector/rector": "^1.2",
        "tightenco/duster": "^3.0",
        "watheqalshowaiter/model-required-fields": "^1.13"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
